{
  "name": "baconbao.github.io",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "test": "eslint --ext .js ./ --color",
    "build": "npm run test && next build",
    "start": "next start",
    "export": "next export",
    "predeploy": "rm -rf ./out && npm run build && npm run export",
    "deploy": "git log --pretty=format:\"%H\" -n 1 > ./out/master.commithash && gh-pages -d out -t true"
  },
  "dependencies": {
    "gh-pages": "^3.1.0",
    "next": "9.5.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-ga": "^3.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.1.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-config-react-tools": "^1.1.7",
    "eslint-plugin-flowtype": "^4.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^4.0.2"
  }
}
